{
  "name": "to-csv",
  "version": "0.0.0",
  "description": "Convert XLS, XLSX, and ODT to CSV",
  "homepage": "http://github.com/uci-soe/to-csv",
  "author": {
    "name": "Rhett Lowe",
    "email": "rhett@uci.edu",
    "url": "https://github.com/rhettl"
  },
  "files": [
    "lib"
  ],
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/index.js",
  "keywords": [
    "xls",
    "xlsx",
    "ods",
    "tsv",
    "csv",
    "json",
    "rest"
  ],
  "devDependencies": {
    "gulp": "^3.6.0",
    "gulp-eslint": "^0.15.0",
    "gulp-exclude-gitignore": "^1.0.0",
    "gulp-istanbul": "^0.9.0",
    "gulp-mocha": "^2.0.0",
    "gulp-nsp": "^0.4.5",
    "gulp-plumber": "^1.0.0",
    "nsp": "^2.2.0"
  },
  "scripts": {
    "test": "npm run istanbul",
    "prepublish": "npm run lint && npm run nsp",
    "start": "node ./bin/www.js",

    "nsp": "nsp check",
    "istanbul": "istanbul cover --include-all-sources -i lib/**/*.js --print none --report text --report text-summary _mocha",
    "lint": "eslint **/*.js"
  },
  "private": true,
  "dependencies": {
    "csv": "^0.4.6",
    "csv-sniffer": "0.0.4",
    "csvrow": "^0.1.0",
    "exceljs": "^0.2.3",
    "mime": "^1.3.4",
    "mmmagic": "^0.4.0",
    "q": "^1.4.1",
    "restify": "^4.0.2",
    "restify-errors": "^3.0.0",
    "stream-body-parser": "0.0.1",
    "xlsx": "^0.8.0"
  }
}
